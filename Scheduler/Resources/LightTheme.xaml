<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<SolidColorBrush x:Key="Dark gray" Color="White"/>
	<SolidColorBrush x:Key="Mid gray" Color="#BFBFBF"/>
	<SolidColorBrush x:Key="Light gray" Color="#E5E5E5"/>
